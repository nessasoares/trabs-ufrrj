public class CriarComodoComposto_TS {

	public void execute(String desc, ArrayList<String> comodos){
		try {
			ComodoCompostoDAO.insertComodoComposto(desc, comodos);
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
